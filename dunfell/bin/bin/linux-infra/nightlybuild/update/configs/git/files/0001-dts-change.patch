@@ -40,22 +40,22 @@
 	leds {
 		compatible = "gpio-leds";
 		hps0 {
-			label = "hps_led0";
+			label = "hps_led8";
 			gpios = <&gpio1 15 1>;
 		};
 
 		hps1 {
-			label = "hps_led1";
+			label = "hps_led9";
 			gpios = <&gpio1 14 1>;
 		};
 
 		hps2 {
-			label = "hps_led2";
+			label = "hps_leda";
 			gpios = <&gpio1 13 1>;
 		};
 
 		hps3 {
-			label = "hps_led3";
+			label = "hps_ledb";
 			gpios = <&gpio1 12 1>;
 		};
 	};
@@ -76,7 +76,7 @@
 };
 
